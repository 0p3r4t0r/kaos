{
  "name": "kaos",
  "description": "Kaos Browser Game",
  "engines": {
    "node": ">=20.10.0"
  },
  "author": "Brian Sutherland <bsuth>",
  "contributors": [
    "Robert Sutherland <0p3r4t0r>"
  ],
  "maintainers": [
    "Robert Sutherland <0p3r4t0r>"
  ],
  "version": "0.2.0",
  "license": "GPL-3.0",
  "homepage": "https://github.com/0p3r4t0r/kaos",
  "scripts": {
    "analyze": "webpack --config webpack.config.production.js --profile --json > stats.json",
    "dev": "webpack-dev-server --config webpack.config.development.js",
    "build": "webpack --config webpack.config.production.js",
    "build:dev": "webpack --config webpack.config.development.js",
    "serve": "npx http-server -o dist",
    "serve:dev": "npx http-server -o devdist",
    "start": "npm run build; npm run serve",
    "start:dev": "npm run build:dev; npm run serve:dev",
    "deploy": "npm run build; git subtree push --prefix dist origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0p3r4t0r/kaos"
  },
  "dependencies": {
    "hammerjs": "2.0.8",
    "swiper": "11.0.05",
    "vue": "3.3.11",
    "vue-i18n": "9.8.0",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@babel/preset-env": "7.23.6",
    "autoprefixer": "10.4.16",
    "babel-loader": "9.1.3",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "eslint": "8.55.0",
    "eslint-plugin-vue": "9.19.2",
    "html-webpack-plugin": "5.5.4",
    "mini-css-extract-plugin": "2.7.6",
    "node-sass": "9.0.0",
    "postcss-loader": "7.3.3",
    "sass-loader": "13.3.2",
    "vue-loader": "17.3.1",
    "vue-style-loader": "4.1.3",
    "webpack": "5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  }
}
